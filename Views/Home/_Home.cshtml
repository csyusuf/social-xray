<!-- ko with: home -->
@using SocialXray.Models
@model SocialXray.Models.ReportViewModels
<script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
<script type="text/javascript">
      google.charts.load("current", {packages:["corechart"]});
      google.charts.setOnLoadCallback(drawChart);
    function drawChart() {
          var keywordsPopularity = @Html.Raw(Json.Encode(Model.KeywordsPopularity));
        var data = google.visualization.arrayToDataTable(keywordsPopularity);

        var options = {
          title: 'My Daily Activities',
          is3D: true,
        };

        var chart = new google.visualization.PieChart(document.getElementById('piechart_3d'));
        chart.draw(data, options);
      }
</script>
<div class="jumbotron">
    <h1>Social X-ray</h1>
</div>
<div class="row">
    <div class="col-md-4">
        <label>First Keyword</label>
        <br />
        <input type="text" name="twit_keywor1" value="@Model.Test"/>
    </div>
    <div class="col-md-4">
        <label> Second Keyword</label>
        <br />
        <input type="text" name="twit_keywor2" />
    </div>
</div>
<div class="row" style="margin-top: 10px">
    <div class="col-md-4">
        <input type="submit" name="btnSearch" value="Search" class="btn-info" />
    </div>
</div>
<div class="row" style="margin-top:10px">
    <div id="piechart_3d" style="width: 900px; height: 500px;">
        
    </div>
</div>